<div class="container">
  <div class="row">
    <div class="col-xs-4 col-xs-offset-3">
      <h1>Details about <%= @school.name %> School</h1>
      <p>Here are the members that belong to this school</p>
      <ul>
        <% @school.members.each do |member| %>
          <li><%= member.name %></li>
        <% end %>
        <%= link_to 'Back to member index', members_path, class: "btn btn-primary"  %>
      </ul>
    </div>
  </div>
</div>
